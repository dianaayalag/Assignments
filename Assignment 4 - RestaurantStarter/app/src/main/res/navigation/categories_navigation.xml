<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/categories"
  app:startDestination="@id/categoriesFragment">

  <fragment
    android:id="@+id/categoriesFragment"
    android:name="com.emedinaa.kotlinapp.CategoriesFragment"
    android:label="fragment_categories"
    tools:layout="@layout/fragment_categories" >
    <action
      android:id="@+id/action_categoriesFragment_to_dishesFragment2"
      app:destination="@id/dishesFragment2" />
  </fragment>
  <fragment
    android:id="@+id/dishesFragment2"
    android:name="com.emedinaa.kotlinapp.DishesFragment"
    android:label="fragment_dishes"
    tools:layout="@layout/fragment_dishes" >
    <action
      android:id="@+id/action_dishesFragment2_to_dishFragment"
      app:destination="@id/dishFragment" />
  </fragment>
  <fragment
    android:id="@+id/dishFragment"
    android:name="com.emedinaa.kotlinapp.DishFragment"
    android:label="fragment_dish"
    tools:layout="@layout/fragment_dish" />
</navigation>